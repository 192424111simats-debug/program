#include <stdio.h>
int main() {
    int n, i, num;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d numbers:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("\nResult:\n");
    for (i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) {
            printf("%d is Even\n", arr[i]);
        } else {
            printf("%d is Odd\n", arr[i]);
        }
    }
    return 0;
}
OUTPUT :
Enter the number of elements: 5
Enter 5 numbers:
5 4 1 8 3

Result:
5 is Odd
4 is Even
1 is Odd
8 is Even
3 is Odd
